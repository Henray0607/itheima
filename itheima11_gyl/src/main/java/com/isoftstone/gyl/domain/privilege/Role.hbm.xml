<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.isoftstone.gyl.domain.privilege.Role" table="role">
		<id name="rid" type="java.lang.Long" length="5">
			<column name="rid"></column>
			<generator class="increment"></generator>
		</id>
		<property name="pid" type="java.lang.Long" length="5"></property>		
		<property name="name" length="20"></property>
		<property name="isParent" type="java.lang.Boolean"></property>
		<set name="users" lazy="false" table="user_role" inverse="true">
			<key>
				<column name="rid"></column>
			</key>
			<many-to-many class="com.isoftstone.gyl.domain.basedata.User" column="uid"></many-to-many>
		</set>
	</class>
</hibernate-mapping>